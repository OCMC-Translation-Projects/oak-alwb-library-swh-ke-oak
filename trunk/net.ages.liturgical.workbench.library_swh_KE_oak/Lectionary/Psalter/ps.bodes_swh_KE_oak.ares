bode9.title = ""
bode9.version = ""
bode9_mag.book = bible_swh_KE_oak.Luke
bode9_mag.heading1 = ""
bode9_mag.chapverse = "(1:47-55)"
bode9_mag.heading2 = ""
bode9_mag.intro = ""
bode9_mag.v1.number = "(9a:1)"
bode9_mag.v1.text = "Moyo wangu wamwadhimisha Bwana, na roho yangu imemfurahia Mungu, Mwokozi wangu."
bode9_mag.v2.number = "(9a:2)"
bode9_mag.v2.text = "Kwa kuwa ameutazama unyonge wa mjakazi wake. Kwa maana, tokea sasa vizazi vyote wataniita mbarikiwa;"
bode9_mag.v3.number = "(9a:3)"
bode9_mag.v3.text = "kwa kuwa Mwenye nguvu amenitendea makuu, na jina lake ni takatifu. Na rehema zake hudumu vizazi hata vizazi kwa hao wanaomcha."
bode9_mag.v4.number = "(9a:4)"
bode9_mag.v4.text = "Amefanya nguvu kwa mkono wake; amewatawanya walio na kiburi katika mawazo ya mioyo yao;"
bode9_mag.v5.number = "(9a:5)"
bode9_mag.v5.text = "amewaangusha wakuu katika viti vyao vya enzi; na wanyonge amewakweza. Wenye njaa amewashibisha mema, na wenye mali amewaondoa mikono mitupu."
bode9_mag.v6.number = "(9a:6)"
bode9_mag.v6.text = "Amemsaidia Israeli, mtumishi wake; ili kukumbuka rehema zake; kama alivyowaambia baba zetu, Ibrahimu na uzao wake hata milele."
bode9.book = bible_swh_KE_oak.Luke
bode9.heading1 = ""
bode9.chapverse = "(Sura ya 1)"
bode9.heading2 = ""
bode9.intro = ""
bode9.v1.number = "(9b:1)"
bode9.v1.text = "Atukuzwe Bwana, Mungu wa Israeli, kwa kuwa amewajia watu wake, na kuwakomboa."
bode9.v2.number = "(9b:2)"
bode9.v2.text = "Ametusimamishia pembe ya wokovu katika mlango wa Daudi, mtumishi wake."
bode9.v3.number = "(9b:3)"
bode9.v3.text = "Kama alivyosema tangu mwanzo kwa kinywa cha manabii wake watakatifu;"
bode9.v4.number = "(9b:4)"
bode9.v4.text = "tuokolewe na adui zetu na mikononi mwao wote wanaotuchukia;"
bode9.v5.number = "(9b:5)"
bode9.v5.text = "ili kuwatendea rehema baba zetu, na kulikumbuka agano lake takatifu,"
bode9.v6.number = "(9b:6)"
bode9.v6.text = "uapo aliomwapia Ibrahimu, baba yetu, ya kwamba atatujalia sisi, tuokoke mikononi mwa adui zetu"
bode9.v7.number = "(9b:7)"
bode9.v7.text = "na kumwabudu pasipo hofu, kwa utakatifu na kwa haki mbele zake siku zetu zote."
bode9.v8.number = "(9b:8)"
bode9.v8.text = "Nawe, mtoto, utaitwa nabii wake Aliye juu, kwa maana utatangulia mbele za uso wa Bwana umtengenezee njia zake;"
bode9.v9.number = "(9b:9)"
bode9.v9.text = "uwajulishe watu wake wokovu, katika kusamehewa dhambi zao. Kwa njia ya rehema za Mungu wetu,"
bode9.v10.number = "(9b:10)"
bode9.v10.text = "ambazo kwa hizo mwangaza utokao juu umetufikia, kuwaangaza wakaao katika giza na uvuli wa mauti,"
bode9.v11.number = "(9b:11)"
bode9.v11.text = "na kuiongoza miguu yetu kwenye njia ya amani."

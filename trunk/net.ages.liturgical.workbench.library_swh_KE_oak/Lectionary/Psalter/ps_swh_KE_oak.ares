psa127.title = "Psa(127)"
psa127.version = ""
psa127.text = ""
psa127.v1.number = ""
psa127.v1.text = "Heri kila mtu amchaye Bwana , aendaye katika kila njia yake."
psa127.v2.number = ""
psa127.v2.text = " Taabu ya mikono yako hakika utaila: utakuwa heri, na kuwa vyema."
psa127.v3.number = ""
psa127.v3.text = "Mkeo atakua kama mzabibu uzaao, nyumbani mwa nyumba yako wanao kama miche ya mizetuini, wakizunguka meza yako."
psa127.v4.number = ""
psa127.v4.text = "Tazama utambarikiwa hivyo, yule amchaye Bwana."
psa127.v5.number = ""
psa127.v5.text = " Bwana atakubariki toka Sayuni; uone heri za Yerusalem siku zote za maisha yako."
psa127.v6.number = ""
psa127.v6.text = "Na ukawaone wana wa wanao. amani ikae na Israeli."


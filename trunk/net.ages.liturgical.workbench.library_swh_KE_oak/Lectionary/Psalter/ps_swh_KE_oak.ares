psa127.title = "Psa 127"
psa127.version = ""
psa127.text = ""
psa127.v1.number = ""
psa127.v1.text = "Heri kila mtu amchaye Bwana , aendaye katika kila njia yake."
psa127.v2.number = ""
psa127.v2.text = " Taabu ya mikono yako hakika utaila: utakuwa heri, na kuwa vyema."
psa127.v3.number = ""
psa127.v3.text = "Mkeo atakua kama mzabibu uzaao, nyumbani mwa nyumba yako wanao kama miche ya mizetuini, wakizunguka meza yako."
psa127.v4.number = ""
psa127.v4.text = "Tazama utambarikiwa hivyo, yule amchaye Bwana, hivyo Bwana  atakubariki toka Sayuni; uone heri za Yerusalem siku zote za maisha yako; Naam ukawaone wana wa wanao. Amani ikae kwa Israeli."
psa127.v5.number = ""
psa127.v5.text = "Umbarikiwe ni ufalme wa Baba na Mwana na Roho Mtakatifu, sasa na siku zote hata milele na milele."

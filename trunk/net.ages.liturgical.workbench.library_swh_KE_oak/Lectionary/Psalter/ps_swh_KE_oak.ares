psa116.title = "Zaburi ya 116 (117)."
psa116.version = ""
psa116.text = "Haleluya. Enyi mataifa yote, msifuni BWANA, enyi watu wote, mhimidini. Maana fadhili zake kwetu sisi ni kuu, na uaminifu wa BWANA ni wa milele."
psa116.v1.number = "(116:1)"
psa116.v1.text = "Haleluya. Enyi mataifa yote, msifuni BWANA, enyi watu wote, mhimidini."

psa99.title = "Zaburi ya 99 (100)."
psa99.version = ""
psa99.text = "Mfanyieni BWANA shangwe, dunia yote; mtumikieni BWANA kwa furaha; njoni mbele zake kwa kuimba; jueni kwamba BWANA ndiye Mungu; ndiye aliyetuumba na sisi tu watu wake; tu watu wake, na kondoo wa malisho yake. Ingieni malangoni mwake kwa kushukuru; nyuani mwake kwa kusifu; mshukuruni, lihimidini jina lake; kwa kuwa BWANA ndiye mwema; rehema zake ni za milele; na uaminifu wake vizazi na vizazi."
psa99.v1.number = "(99:1)"
psa99.v1.text = "Mfanyieni BWANA shangwe, dunia yote;"

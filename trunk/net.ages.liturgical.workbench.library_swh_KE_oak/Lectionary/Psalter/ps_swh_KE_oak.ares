psa53.v6.number = "(53:6)"
psa53.v6.text = "Tazama, Mungu ndiye anayenisaidia; Bwana ndiye anayenitegemeza nafsi yangu."
psa38.v2.text = "Nalisema, Nitazitunza njia zangu nisije nikakosa kwa ulimi wangu; nitajitia lijamu kinywani, maadamu mtu mbaya yupo mbele yangu."
psa40.v2.text = "Heri amkumbukaye mnyonge; BWANA atamwokoa siku ya taabu."
psa41.v2.text = "Kama ayala aioneavyo shauku mito ya maji, vivyo hivyo nafsi yangu inakuonea shauku, Ee Mungu."
psa41.v2.number = "(41:2)"
psa51.v3.text = "Kwa nini kujisifia uovu, Ewe jabari? Wema wa Mungu upo siku zote."
psa51.v2.number = "(51:2)"
psa51.v3.number = "(51:3)"
psa52.v2.text = "Mpumbavu amesema moyoni, Hakuna Mungu; wametenda uovu wa ufisadi na kuchukiza, hakuna atendaye mema."
psa52.v2.number = "(52:2)"
psa55.v2.number = "(55:2)"
psa55.v2.text = "Mungu, unirehemu, maana yuko atakaye kunimeza, mchana kutwa ananionea akileta vita."
psa56.v2.text = "Unirehemu, Ee Mungu, unirehemu mimi, maana nafsi yangu imekukimbilia Wewe. Nitaukimbilia uvuli wa mbawa zako, hata misiba hii itakapopita."
psa56.v2.number = "(56:2)"
psa57.v2.text = "Ni kweli, enyi wakuu, mnanena haki? Enyi wanadamu, mnahukumu kwa adili?"
psa57.v2.number = "(57:2)"
psa59.v3.text = "Ee Mungu, umetutupa na kututawanya, umekuwa na hasira, uturudishe tena."
psa59.v2.number = "(59:2)"
psa59.v3.number = "(59:3)"
psa60.v2.text = "Ee Mungu, ukisikie kilio changu, uyasikilize maombi yangu."
psa60.v2.number = "(60:2)"
psa61.v2.text = "Nafsi yangu yamngoja Mungu peke yake kwa kimya, wokovu wangu hutoka kwake."
psa61.v2.number = "(61:2)"
psa69.v2.number = "(69:2)"
psa69.v2.text = "Ee Mungu, uniokea, Ee BWANA, unisaidie hima."
psa74.v2.number = "(74:2)"
psa74.v11.number = "(74:11)"
psa74.v2.text = "Ee Mungu, twakushukuru. Twakushukuru kwa kuwa Jina lako li karibu;"
psa74.v11.text = "Pembe zote za wasio haki nitazikata, na pembe za mwenye haki zitainuka."
psa80.v2.number = "(80:2)"
psa80.v2.text = "Mwimbieni Mungu, nguvu zetu, nyimbo za furaha, mshangilieni Mungu wa Yakobo."
psa82.v2.number = "(82:2)"
psa82.v2.text = "Ee Mungu, usistarehe, Ee Mungu, usinyamae, wala usitulie."
psa83.v2.number = "(83:2)"
psa83.v2.text = "Maskani zako zapendeza kama nini, Ee BWANA wa majeshi!"
psa44.v10b.text = "Mkono wako wa kuume amesimama malkia, amevaa dhahabu ya Ofiri."
psa35.v11a.text = "Uwadumishie wakujuao fadhili zako,"

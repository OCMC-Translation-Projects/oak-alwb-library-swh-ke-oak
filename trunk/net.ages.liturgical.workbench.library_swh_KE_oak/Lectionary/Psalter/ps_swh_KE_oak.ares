psa99.v2.text = "mtumikieni BWANA kwa furaha; njoni mbele zake kwa kuimba;"
psa99.v3.number = "(99:3)"
psa99.v3.text = "jueni kwamba BWANA ndiye Mungu; ndiye aliyetuumba na sisi tu watu wake; tu watu wake, na kondoo wa malisho yake."
psa99.v4.number = "(99:4)"
psa99.v4.text = "Ingieni malangoni mwake kwa kushukuru; nyuani mwake kwa kusifu; mshukuruni, lihimidini jina lake;"
psa99.v4a.number = "(99:4a)"
